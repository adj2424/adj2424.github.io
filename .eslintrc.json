{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": 0,
        "no-plusplus": 0,
        "comma-dangle": 0,
        "max-len": ["error", { "code": 120 }],
        "no-restricted-syntax": 0
        
        
    }
}
